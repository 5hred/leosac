<root>
    <users>
        <user>
            <name>Toto</name>
        </user>

        <user>
            <name>Llamasticot</name>
            <enabled>false</enabled>
        </user>

        <user>
            <name>llama1</name>
            <validity_start>01/01/2015 00:00</validity_start>
            <validity_end>01/01/2020 00:00</validity_end>
        </user>

        <user>
            <name>llama2</name>
            <validity_start>01/01/2015 00:00</validity_start>
            <validity_end>01/02/2015 00:00</validity_end>
        </user>
    </users>
    <!--
    Maps WiegandCard to user
    -->
    <credentials>
        <map>
            <user>Toto</user>
            <WiegandCardPin>
                <card>cc:dd:ee:ff</card>
                <pin>1234</pin>
            </WiegandCardPin>
        </map>
        <map>
            <user>Llamasticot</user>
            <WiegandCard>aa:bb:cc:dd</WiegandCard>
        </map>
        <map>
            <user>llama1</user>
            <WiegandCard>cc:dd:ee:ff</WiegandCard>
        </map>
        <map>
            <user>llama2</user>
            <PINCode>1234</PINCode>
        </map>
    </credentials>

    <schedules>
        <schedule>
            <name>full_sunday</name>
            <sunday>
                <start>00:00</start>
                <end>23:59</end>
            </sunday>
        </schedule>
    </schedules>

    <schedules_mapping>
        <map>
            <schedule>full_sunday</schedule>
            <user>Toto</user>
            <user>Llamasticot</user>
            <user>llama1</user>
            <user>llama2</user>
        </map>
    </schedules_mapping>

</root>
