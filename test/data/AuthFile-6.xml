<root>
    <users>
        <user>
            <name>Toto</name>
        </user>
    </users>
    <!--
    Maps WiegandCard to user
    -->
    <credentials>
        <map>
            <user>Toto</user>
            <WiegandCardPin>
                <card>cc:dd:ee:ff</card>
                <pin>1234</pin>
            </WiegandCardPin>
        </map>
    </credentials>

    <schedules>
        <schedule>
            <name>full_sunday</name>
            <sunday>
                <start>00:00</start>
                <end>23:59</end>
            </sunday>
        </schedule>
    </schedules>

    <schedules_mapping>
        <map>
            <schedule>full_sunday</schedule>
            <user>Toto</user>
        </map>
    </schedules_mapping>

</root>
